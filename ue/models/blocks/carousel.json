{
  "definitions": [
    {
      "title": "Carousel",
      "id": "carousel",
      "model": "carousel",
      "filter": "carousel",
      "plugins": {
        "da": {
          "rows": 1,
          "columns": 1
        }
      }
    },
    {
      "title": "Carousel Slide - V2",
      "id": "carousel-item",
      "model": "carousel-item",
      "plugins": {
        "da": {
          "rows": 1,
          "columns": 1,
          "fields": [
            { "name": "backgroundImage", "selector": "div>picture>img:nth-child(3)[src]"},
            { "name": "backgroundImageAlt", "selector": "div>picture>img:nth-child(3)[alt]"},
            { "name": "text", "selector": "div:nth-child(2)>p"}
          ]
        }
      }
    }
  ],
  "models": [
    {
      "id": "carousel-item",
      "fields": [
        {
          "component": "reference",
          "valueType": "string",
          "name": "backgroundImage",
          "label": "Background Image",
          "multi": false
        },
        {
          "component": "text",
          "valueType": "string",
          "name": "backgroundImageAlt",
          "label": "Background Image Alt"
        },
        {
          "component": "richtext",
          "name": "text",
          "label": "Text",
          "valueType": "string"
        }
      ]
    }
  ],
  "filters": [
    {
      "id": "carousel",
      "components": ["carousel-item"]
    }
  ]
}
