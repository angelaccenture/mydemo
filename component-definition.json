{
  "groups": [
    {
      "title": "Default Content",
      "id": "default",
      "components": [
        {
          "title": "Text",
          "id": "text",
          "model": "text",
          "plugins": {
            "da": {
              "name": "text",
              "type": "text"
            }
          }
        },
        {
          "title": "Image",
          "id": "image",
          "model": "image",
          "plugins": {
            "da": {
              "name": "image",
              "type": "image",
              "fields": [
                {
                  "name": "imageAlt",
                  "selector": "img[alt]"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "title": "Sections",
      "id": "sections",
      "components": [
        {
          "title": "Section Metadata",
          "id": "section",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>"
            }
          },
          "filter": "section",
          "model": "section"
        }
      ]
    },
    {
      "title": "Blocks",
      "id": "blocks",
      "components": [
        {
          "title": "Accordion",
          "id": "accordion",
          "plugins": {
            "da": {
              "name": "accordion",
              "unsafeHTML": "<div class=\"accordion\"></div>"
            }
          }
        },
        {
          "title": "Accordion Item",
          "id": "accordion-item",
          "plugins": {
            "da": {
              "name": "accordion-item",
              "unsafeHTML": "<div><div>Summary</div><div>Text Area</div></div>"
            }
          }
        },
        {
          "title": "Advanced Tabs - AK",
          "id": "advanced-tabs",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"tabs\"><div><div><p></p></div><div></div></div></div>"
            }
          }
        },
        {
          "title": "Tabs Item",
          "id": "tabs-item",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><p></p></div><div></div></div>"
            }
          }
        },
        {
          "title": "Card",
          "id": "card",
          "model": "card",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"card\"><div><div><p><picture><source type=\"image/webp\" srcset=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\" media=\"(min-width: 600px)\"><source type=\"image/webp\" srcset=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\"><source type=\"image/jpeg\" srcset=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\" media=\"(min-width: 600px)\"><img loading=\"lazy\" alt=\"Alt Text\" src=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\" width=\"3840\" height=\"2160\"></picture></p><p><strong>Unmatched speed</strong></p><p>AEM is the fastest way to publish, create, and serve websites.</p></div></div></div>",
              "fields": [
                {
                  "name": "image",
                  "selector": "div>div>picture>img[src]"
                },
                {
                  "name": "imageAlt",
                  "selector": "div>div>picture>img[alt]"
                },
                {
                  "name": "text",
                  "selector": "div>div>p"
                }
              ]
            }
          }
        },
        {
          "title": "Carousel",
          "id": "carousel",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"carousel\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div>"
            }
          }
        },
        {
          "title": "Carousel Slide",
          "id": "carousel-item",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div>"
            }
          }
        },
        {
          "title": "Columns",
          "id": "columns",
          "plugins": {
            "da": {
              "name": "columns",
              "rows": 1,
              "columns": 2,
              "behaviour": "columns"
            }
          }
        },
        {
          "title": "Columns Row",
          "id": "columns-row",
          "plugins": {
            "da": {
              "name": "columns-row",
              "behaviour": "columns-row"
            }
          }
        },
        {
          "title": "Columns Cell",
          "id": "columns-cell",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>",
              "behaviour": "columns-cell"
            }
          }
        },
        {
          "title": "Fragment",
          "id": "fragment",
          "model": "fragment",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"fragment\"><div><div><p><a href=\"\">Fragment URL</a></p></div></div></div>",
              "fields": [
                {
                  "name": "url",
                  "selector": "div>div>p>a[href]"
                },
                {
                  "name": "urlText",
                  "selector": "div>div>p>a"
                }
              ]
            }
          }
        },
        {
          "title": "Hero",
          "id": "hero",
          "model": "hero",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"hero\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture><h1></h1></div></div></div>",
              "fields": [
                {
                  "name": "image",
                  "selector": "div>div>picture>img[src]"
                },
                {
                  "name": "imageAlt",
                  "selector": "div>div>picture>img[alt]"
                },
                {
                  "name": "text",
                  "selector": "div>div>h1"
                }
              ]
            }
          }
        },
        {
          "title": "Table - AK",
          "id": "table",
          "plugins": {
            "da": {
              "name": "table",
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "Tabs",
          "id": "tabs",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"tabs\"><div><div><p></p></div><div></div></div></div>"
            }
          }
        },
        {
          "title": "Tabs Item",
          "id": "tabs-item",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><p></p></div><div></div></div>"
            }
          }
        },
        {
          "title": "Video",
          "id": "video",
          "plugins": {
            "da": {
              "name": "video",
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "YouTube - AK",
          "id": "youtube",
          "plugins": {
            "da": {
              "name": "youtube",
              "rows": 1,
              "columns": 1
            }
          }
        }
      ]
    }
  ]
}