{
  "groups": [
    {
      "title": "Default Content",
      "id": "default",
      "components": [
        {
          "title": "Text",
          "id": "text",
          "plugins": {
            "da": {
              "name": "text",
              "unsafeHTML": "<p>Text goes here</p>"
            }
          }
        },
        {
          "title": "Image",
          "id": "image",
          "model": "image",
          "plugins": {
            "da": {
              "name": "image",
              "type": "image",
              "fields": [
                {
                  "name": "imageAlt",
                  "selector": "img[alt]"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "title": "Sections",
      "id": "sections",
      "components": [
        {
          "title": "Section",
          "id": "section",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>"
            }
          },
          "filter": "section",
          "model": "section"
        }
      ]
    },
    {
      "title": "Blocks",
      "id": "blocks",
      "components": [
        {
          "title": "Accordion",
          "id": "accordion",
          "model": "accordion",
          "filter": "accordion",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"accordion\"></div>"
            }
          }
        },
        {
          "title": "Accordion Item",
          "id": "accordion-item",
          "model": "accordion-item",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div>Summary</div><div>Text Area</div></div>",
              "fields": [
                {
                  "name": "summary",
                  "selector": "div>div:nth-child(1)"
                },
                {
                  "name": "text",
                  "selector": "div>div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Carousel",
          "id": "carousel",
          "model": "advanced-carousel",
          "filter": "advanced-carousel",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "Carousel Slide",
          "id": "carousel-item",
          "model": "carousel-item",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "name": "backgroundImage",
                  "selector": "div>picture>img:nth-child(3)[src]"
                },
                {
                  "name": "backgroundImageAlt",
                  "selector": "div>picture>img:nth-child(3)[alt]"
                },
                {
                  "name": "text",
                  "selector": "div:nth-child(2)>p"
                }
              ]
            }
          }
        },
        {
          "title": "Tabs",
          "id": "advanced-tabs",
          "model": "advanced-tabs",
          "filter": "advanced-tabs",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"advanced-tabs\"><div><ul><li>Tab 1</li><li>Tab 2</li><li>Tab 3</li></ul></div></div>",
              "fields": [
                {
                  "name": "tablist-one",
                  "selector": "div>div>ul>li"
                },
                {
                  "name": "tablist-two",
                  "selector": "div>div>ul>li:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Button",
          "id": "button",
          "plugins": {
            "xwalk": {
              "page": {
                "resourceType": "core/franklin/components/block/v1/block",
                "template": {
                  "name": "Button",
                  "model": "button"
                }
              }
            }
          }
        },
        {
          "title": "Card",
          "id": "card",
          "model": "card",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"card\"><div><div><p><picture><source type=\"image/webp\" srcset=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\" media=\"(min-width: 600px)\"><source type=\"image/webp\" srcset=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\"><source type=\"image/jpeg\" srcset=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\" media=\"(min-width: 600px)\"><img loading=\"lazy\" alt=\"Alt Text\" src=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\" width=\"3840\" height=\"2160\"></picture></p><p><strong>Unmatched speed</strong></p><p>AEM is the fastest way to publish, create, and serve websites.</p></div></div></div>",
              "fields": [
                {
                  "name": "image",
                  "selector": "div>div>picture>img[src]"
                },
                {
                  "name": "imageAlt",
                  "selector": "div>div>picture>img[alt]"
                },
                {
                  "name": "title",
                  "selector": "div>div>p:nth-child(2)>strong"
                },
                {
                  "name": "text",
                  "selector": "div>div>p:nth-child(3)"
                }
              ]
            }
          }
        },
        {
          "title": "Form",
          "id": "form",
          "plugins": {
            "xwalk": {
              "page": {
                "resourceType": "core/franklin/components/block/v1/block",
                "template": {
                  "name": "Form",
                  "model": "form"
                }
              }
            }
          }
        },
        {
          "title": "Fragment",
          "id": "fragment",
          "model": "fragment",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"fragment\"><div><div><p><a href=\"\">Fragment URL</a></p></div></div></div>",
              "fields": [
                {
                  "name": "url",
                  "selector": "div>div>p>a[href]"
                },
                {
                  "name": "urlText",
                  "selector": "div>div>p>a"
                }
              ]
            }
          }
        },
        {
          "title": "Hero",
          "id": "hero",
          "model": "hero",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"hero\"><div><div><picture><source type=\"image/webp\" srcset=\"https://content.da.live/aemsites/author-kit/media/bg-image.jpg\" media=\"(min-width: 600px)\"><source type=\"image/webp\" srcset=\"https://content.da.live/aemsites/author-kit/media/bg-image.jpg\"><source type=\"image/jpeg\" srcset=\"https://content.da.live/aemsites/author-kit/media/bg-image.jpg\" media=\"(min-width: 600px)\"><img loading=\"lazy\" alt=\"Alt Text\" src=\"https://content.da.live/aemsites/author-kit/media/bg-image.jpg\" width=\"3840\" height=\"2160\"></picture></div></div><div><div><h1>Author Kit</h1><p>Powerfully simple experiences.</p></div></div></div>",
              "fields": [
                {
                  "name": "image",
                  "selector": "div:nth-child(1)>picture>img[src]"
                },
                {
                  "name": "imageAlt",
                  "selector": "div:nth-child(1)>picture>img[alt]"
                },
                {
                  "name": "title",
                  "selector": "div:nth-child(2)>div>h1"
                },
                {
                  "name": "text",
                  "selector": "div:nth-child(2)>div>p"
                }
              ]
            }
          }
        },
        {
          "title": "Fragment",
          "id": "fragment",
          "model": "fragment",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"fragment\"><div><div><p><a href=\"\">Fragment URL</a></p></div></div></div>",
              "fields": [
                {
                  "name": "url",
                  "selector": "div>div>p>a[href]"
                },
                {
                  "name": "urlText",
                  "selector": "div>div>p>a"
                }
              ]
            }
          }
        },
        {
          "title": "Table",
          "id": "table",
          "plugins": {
            "xwalk": {
              "page": {
                "resourceType": "core/franklin/components/block/v1/block",
                "template": {
                  "name": "Table",
                  "model": "table",
                  "filter": "table"
                }
              }
            }
          }
        },
        {
          "title": "Row",
          "id": "table-row",
          "plugins": {
            "xwalk": {
              "page": {
                "resourceType": "core/franklin/components/block/v1/block/item",
                "template": {
                  "name": "Row",
                  "model": "table-row",
                  "column1text": "Column1"
                }
              }
            }
          }
        },
        {
          "title": "Row",
          "id": "table-col-2",
          "plugins": {
            "xwalk": {
              "page": {
                "resourceType": "core/franklin/components/block/v1/block/item",
                "template": {
                  "name": "Row",
                  "model": "table-col-2",
                  "column1text": "Column1",
                  "column2text": "Column2"
                }
              }
            }
          }
        },
        {
          "title": "Row",
          "id": "table-col-3",
          "plugins": {
            "xwalk": {
              "page": {
                "resourceType": "core/franklin/components/block/v1/block/item",
                "template": {
                  "name": "Row",
                  "model": "table-col-3",
                  "column1text": "Column1",
                  "column2text": "Column2",
                  "column3text": "Column3"
                }
              }
            }
          }
        },
        {
          "title": "Row",
          "id": "table-col-4",
          "plugins": {
            "xwalk": {
              "page": {
                "resourceType": "core/franklin/components/block/v1/block/item",
                "template": {
                  "name": "Row",
                  "model": "table-col-4",
                  "column1text": "Column1",
                  "column2text": "Column2",
                  "column3text": "Column3",
                  "column4text": "Column4"
                }
              }
            }
          }
        },
        {
          "title": "Teaser",
          "id": "teaser",
          "plugins": {
            "xwalk": {
              "page": {
                "resourceType": "core/franklin/components/block/v1/block",
                "template": {
                  "name": "Teaser",
                  "model": "teaser",
                  "key-value": true
                }
              }
            }
          }
        }
      ]
    }
  ]
}