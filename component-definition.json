{
  "groups": [
    {
      "title": "Default Content",
      "id": "default",
      "components": [
        {
          "title": "Text",
          "id": "text",
          "model": "text",
          "plugins": {
            "da": {
              "name": "text",
              "type": "text"
            }
          }
        },
        {
          "title": "Image",
          "id": "image",
          "model": "image",
          "plugins": {
            "da": {
              "name": "image",
              "type": "image",
              "fields": [
                {
                  "name": "imageAlt",
                  "selector": "img[alt]"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "title": "Sections",
      "id": "sections",
      "components": [
        {
          "title": "Section Metadata",
          "id": "section",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>"
            }
          },
          "filter": "section",
          "model": "section"
        }
      ]
    },
    {
      "title": "Blocks",
      "id": "blocks",
      "components": [
        {
          "title": "Accordion",
          "id": "accordion",
          "model": "accordion",
          "filter": "accordion",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 2
            }
          }
        },
        {
          "title": "Accordion Item",
          "id": "accordion-item",
          "model": "accordion-item",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 2,
              "fields": [
                {
                  "name": "summary",
                  "selector": "div>div:nth-child(1)>p"
                },
                {
                  "name": "text",
                  "selector": "div>div:nth-child(2)>p"
                }
              ]
            }
          }
        },
        {
          "title": "Tabs",
          "id": "tabs",
          "model": "tabs",
          "filter": "tabs",
          "plugins": {
            "da": {
              "columns": 1,
              "rows": 2
            }
          }
        },
        {
          "title": "Tabs Item",
          "id": "tabs-item",
          "model": "tabs-item",
          "plugins": {
            "da": {
              "columns": 0,
              "rows": 2,
              "fields": [
                {
                  "name": "name",
                  "selector": "div:nth-child(1)"
                },
                {
                  "name": "content",
                  "selector": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Card",
          "id": "card",
          "model": "card",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"card\"><div><div><p><picture><source type=\"image/webp\" srcset=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\" media=\"(min-width: 600px)\"><source type=\"image/webp\" srcset=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\"><source type=\"image/jpeg\" srcset=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\" media=\"(min-width: 600px)\"><img loading=\"lazy\" alt=\"Alt Text\" src=\"https://sidekick-library--aem-block-collection--adobe.aem.live/media_1300083772e9645ea9463e3ff16e3a87fec824b7c.jpg\" width=\"3840\" height=\"2160\"></picture></p><p><strong>Unmatched speed</strong></p><p>AEM is the fastest way to publish, create, and serve websites.</p></div></div></div>",
              "fields": [
                {
                  "name": "image",
                  "selector": "div>div>picture>img[src]"
                },
                {
                  "name": "imageAlt",
                  "selector": "div>div>picture>img[alt]"
                },
                {
                  "name": "title",
                  "selector": "div>div>p:nth-child(2)>strong"
                },
                {
                  "name": "text",
                  "selector": "div>div>p:nth-child(3)"
                }
              ]
            }
          }
        },
        {
          "title": "Carousel",
          "id": "carousel",
          "model": "carousel",
          "filter": "carousel",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "Carousel Slide",
          "id": "carousel-item",
          "model": "carousel-item",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "name": "backgroundImage",
                  "selector": "div>picture>img:nth-child(3)[src]"
                },
                {
                  "name": "backgroundImageAlt",
                  "selector": "div>picture>img:nth-child(3)[alt]"
                },
                {
                  "name": "text",
                  "selector": "div:nth-child(2)>p"
                }
              ]
            }
          }
        },
        {
          "title": "Columns",
          "id": "columns",
          "model": "columns",
          "filter": "columns",
          "plugins": {
            "da": {
              "name": "columns",
              "rows": 1,
              "columns": 2,
              "behaviour": "columns"
            }
          }
        },
        {
          "title": "Columns Row",
          "id": "columns-row",
          "model": "columns-row",
          "filter": "columns-row",
          "plugins": {
            "da": {
              "name": "columns-row",
              "behaviour": "columns-row"
            }
          }
        },
        {
          "title": "Columns Cell",
          "id": "columns-cell",
          "model": "columns-cell",
          "filter": "columns-cell",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>",
              "behaviour": "columns-cell"
            }
          }
        },
        {
          "title": "Fragment",
          "id": "fragment",
          "model": "fragment",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"fragment\"><div><div><p><a href=\"\">Fragment URL</a></p></div></div></div>",
              "fields": [
                {
                  "name": "url",
                  "selector": "div>div>p>a[href]"
                },
                {
                  "name": "urlText",
                  "selector": "div>div>p>a"
                }
              ]
            }
          }
        },
        {
          "title": "Hero",
          "id": "hero",
          "model": "hero",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"hero\"><div><div><picture><source type=\"image/webp\" srcset=\"https://content.da.live/aemsites/author-kit/media/bg-image.jpg\" media=\"(min-width: 600px)\"><source type=\"image/webp\" srcset=\"https://content.da.live/aemsites/author-kit/media/bg-image.jpg\"><source type=\"image/jpeg\" srcset=\"https://content.da.live/aemsites/author-kit/media/bg-image.jpg\" media=\"(min-width: 600px)\"><img loading=\"lazy\" alt=\"Alt Text\" src=\"https://content.da.live/aemsites/author-kit/media/bg-image.jpg\" width=\"3840\" height=\"2160\"></picture></div></div><div><div><h1>Author Kit</h1><p>Powerfully simple experiences.</p></div></div></div>",
              "fields": [
                {
                  "name": "image",
                  "selector": "div:nth-child(1)>picture>img[src]"
                },
                {
                  "name": "imageAlt",
                  "selector": "div:nth-child(1)>picture>img[alt]"
                },
                {
                  "name": "title",
                  "selector": "div:nth-child(2)>div>h1"
                },
                {
                  "name": "text",
                  "selector": "div:nth-child(2)>div>p"
                }
              ]
            }
          }
        },
        {
          "title": "Schedule",
          "id": "schedule",
          "plugins": {
            "da": {
              "name": "schedule",
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "Table - AK",
          "id": "table",
          "plugins": {
            "da": {
              "name": "table",
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "Tabs",
          "id": "tabs",
          "model": "tabs",
          "filter": "tabs",
          "plugins": {
            "da": {
              "columns": 1,
              "rows": 2
            }
          }
        },
        {
          "title": "Tabs Item",
          "id": "tabs-item",
          "model": "tabs-item",
          "plugins": {
            "da": {
              "columns": 0,
              "rows": 2,
              "fields": [
                {
                  "name": "name",
                  "selector": "div:nth-child(1)"
                },
                {
                  "name": "content",
                  "selector": "div:nth-child(2)"
                }
              ]
            }
          }
        },
        {
          "title": "Video",
          "id": "video",
          "model": "video",
          "plugins": {
            "da": {
              "name": "video",
              "rows": 1,
              "columns": 1,
              "fields": [
                {
                  "name": "videoUrl",
                  "selector": "div>div>p>a[href]"
                },
                {
                  "name": "videoUrlText",
                  "selector": "div>div>p>a"
                },
                {
                  "name": "posterImage",
                  "selector": "div>div>picture>img[src]"
                }
              ]
            }
          }
        },
        {
          "title": "YouTube - AK",
          "id": "youtube",
          "plugins": {
            "da": {
              "name": "youtube",
              "rows": 1,
              "columns": 1
            }
          }
        }
      ]
    }
  ]
}