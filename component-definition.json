{
  "groups": [
    {
      "title": "Default Content",
      "id": "default",
      "components": [
        {
          "title": "Text - Test",
          "id": "text",
          "plugins": {
            "da": {
              "name": "text",
              "type": "text"
            }
          }
        },
        {
          "title": "Image",
          "id": "image",
          "plugins": {
            "da": {
              "name": "image",
              "type": "image"
            }
          }
        }
      ]
    },
    {
      "title": "Sections",
      "id": "sections",
      "components": [
        {
          "title": "Section Metadata",
          "id": "section",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>"
            }
          },
          "filter": "section",
          "model": "section"
        }
      ]
    },
    {
      "title": "Blocks",
      "id": "blocks",
      "components": [
        {
          "title": "Accordion - BC",
          "id": "accordion",
          "plugins": {
            "da": {
              "name": "accordion",
              "rows": 1,
              "columns": 2
            }
          }
        },
        {
          "title": "Accordion Item",
          "id": "accordion-item",
          "plugins": {
            "da": {
              "name": "accordion-item",
              "rows": 2,
              "columns": 0
            }
          }
        },
        {
          "title": "Advanced Tabs - AK",
          "id": "advanced-tabs",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"tabs\"><div><div><p></p></div><div></div></div></div>"
            }
          }
        },
        {
          "title": "Tabs Item",
          "id": "tabs-item",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><p></p></div><div></div></div>"
            }
          }
        },
        {
          "title": "Card - AK",
          "id": "cards",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"cards\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div></div>"
            }
          },
          "model": "cards",
          "filter": "cards"
        },
        {
          "title": "Card Item - AK",
          "id": "card",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div>"
            }
          },
          "model": "card"
        },
        {
          "title": "Carousel",
          "id": "carousel",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"carousel\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div>"
            }
          }
        },
        {
          "title": "Carousel Slide",
          "id": "carousel-item",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div>"
            }
          }
        },
        {
          "title": "Columns - AK",
          "id": "columns",
          "plugins": {
            "da": {
              "name": "columns",
              "rows": 1,
              "columns": 2,
              "behaviour": "columns"
            }
          }
        },
        {
          "title": "Columns Row",
          "id": "columns-row",
          "plugins": {
            "da": {
              "name": "columns-row",
              "behaviour": "columns-row"
            }
          }
        },
        {
          "title": "Columns Cell",
          "id": "columns-cell",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>",
              "behaviour": "columns-cell"
            }
          }
        },
        {
          "title": "Fragment - BC",
          "id": "fragment",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"fragment\"><div><div><p><a href=\"\">Fragment URL</a></p></div></div></div>"
            }
          }
        },
        {
          "title": "Hero",
          "id": "hero",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"hero\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture><h1></h1></div></div></div>"
            }
          }
        },
        {
          "title": "Table - AK",
          "id": "table",
          "plugins": {
            "da": {
              "name": "table",
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "YouTube - AK",
          "id": "youtube",
          "plugins": {
            "da": {
              "name": "youtube",
              "rows": 1,
              "columns": 1
            }
          }
        }
      ]
    }
  ]
}